[Unit]
Description=file manager service

[Service]
Type=simple
WorkingDirectory=/home/jello-backend/deploy/file_manager
ExecStart=/home/jello-backend/deploy/file_manager/file_manager -config config/gcp-staging.json -port=443  1>/dev/null 2>&1 &
Restart=always
RestartSec=10

[Install]
WantedBy=multi-user.target
